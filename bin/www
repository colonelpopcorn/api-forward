#!/usr/bin/env node
"use strict";

//module dependencies
var HttpHelper = require("../build/HttpHelper/HttpHelper").default;
var Server = require("../build/Server/Server").default;
var http = require("http");
var httpServer;
//create http server

var app = Server.bootstrap().app;
var httpPort = HttpHelper.normalizePort(process.env.PORT, 8080);
app.set("port", httpPort);
httpServer = HttpHelper.createServer({
  app,
  httpPort,
  debugFunc: (str) => { console.log(str); }
}, true);
