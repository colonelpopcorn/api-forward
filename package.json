{
  "name": "api-forward",
  "version": "1.0.0",
  "description": "An api to forward requests from fiddles and pens.",
  "main": "app.js",
  "scripts": {
    "build": "tsc -p .",
    "dev": "cross-env NODE-ENV=development nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "heroku-postbuild": "npm run build",
    "lint": "tslint -p .",
    "start": "node ./build/index.js",
    "test": "ts-mocha -p . src/**/*.spec.ts"
  },
  "engines": {
    "node": ">=15.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colonelpopcorn/api-forward.git"
  },
  "author": "Jonathan Ling",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colonelpopcorn/api-forward/issues"
  },
  "homepage": "https://github.com/colonelpopcorn/api-forward#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/expect": "^1.20.3",
    "@types/express": "^4.11.1",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "^5.1.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^14.14.17",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^1.18.4",
    "ts-mocha": "^1.2.0",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0",
    "typescript": "^4.1.3"
  }
}
